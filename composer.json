{
  "name": "scryba/laravel-backup-complete-restore",
  "description": "Complete backup restoration for Spatie Laravel Backup - restores both database and files to their exact locations with consolidated configuration and internal health checks",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "backup",
    "restore",
    "spatie",
    "database",
    "files",
    "complete-restore",
    "health-checks",
    "consolidated-config"
  ],
  "authors": [
    {
      "name": "Michael K. Laweh",
      "email": "contact@michael.laweitech.com",
      "homepage": "https://michael.laweitech.com/",
      "role": "Developer"
    }
  ],
  "funding": [
    {
      "type": "custom",
      "url": "https://michael.laweitech.com/buy-me-a-coffee"
    }
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3|^8.4",
    "illuminate/console": "^10.0|^11.0|^12.0",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/filesystem": "^10.0|^11.0|^12.0",
    "spatie/laravel-backup": "^8.0|^9.0",
    "wnx/laravel-backup-restore": "^1.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0|^11.0",
    "orchestra/testbench": "^8.0|^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "Scryba\\LaravelBackupCompleteRestore\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Scryba\\LaravelBackupCompleteRestore\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Scryba\\LaravelBackupCompleteRestore\\BackupCompleteRestoreServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "version": "1.4.0",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
